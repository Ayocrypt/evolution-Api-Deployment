SERVER_TYPE=http
SERVER_PORT=8081
SERVER_URL=http://localhost:8081
CORS_ORIGIN=*
AUTHENTICATION_API_KEY=evolution-api-key-change-me

# Redis (Docker: redis service)
CACHE_REDIS_ENABLED=true
CACHE_REDIS_URI=redis://redis:6379

# Postgres container credentials (you set these â€“ no "default", this is your password)
POSTGRES_DATABASE=evolution_postgres
POSTGRES_USERNAME=evolution_user
POSTGRES_PASSWORD=my_production_password

# Database (must match above: user, password, db name)
DATABASE_ENABLED=true
DATABASE_PROVIDER=postgresql
DATABASE_CONNECTION_URI=postgresql://evolution_user:my_production_password@evolution-postgres:5432/evolution_postgres?schema=public
DATABASE_CONNECTION_CLIENT_NAME=evolution_exchange

LOG_LEVEL=ERROR,WARN,INFO
LOG_COLOR=true


